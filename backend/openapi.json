{
    "openapi": "3.1.0",
    "info": {
        "title": "youtube-audio-downloader-api",
        "summary": "API to search and download YouTube videos in various audio formats.",
        "contact": {
            "name": "Jordan Shatford",
            "url": "https://github.com/jordanshatford/youtube-audio-downloader"
        },
        "license": {
            "name": "MIT License",
            "url": "https://github.com/jordanshatford/youtube-audio-downloader/blob/main/LICENSE"
        },
        "version": "1.0.0"
    },
    "paths": {
        "/search": {
            "get": {
                "tags": [
                    "search"
                ],
                "summary": "Get Search",
                "operationId": "get_search",
                "security": [
                    {
                        "HTTPBearer": []
                    }
                ],
                "parameters": [
                    {
                        "name": "term",
                        "in": "query",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Term"
                        }
                    },
                    {
                        "name": "results",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 12,
                            "title": "Results"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "$ref": "#/components/schemas/Video-Output"
                                    },
                                    "title": "Response Get Search"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/session": {
            "get": {
                "tags": [
                    "session"
                ],
                "summary": "Get Session",
                "operationId": "get_session",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Session"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "session"
                ],
                "summary": "Delete Session",
                "operationId": "delete_session",
                "responses": {
                    "204": {
                        "description": "Successful Response"
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                },
                "security": [
                    {
                        "HTTPBearer": []
                    }
                ]
            }
        },
        "/downloads": {
            "get": {
                "tags": [
                    "downloads"
                ],
                "summary": "Get Downloads",
                "operationId": "get_downloads",
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "items": {
                                        "$ref": "#/components/schemas/Video-Output"
                                    },
                                    "type": "array",
                                    "title": "Response Get Downloads"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden"
                    }
                },
                "security": [
                    {
                        "HTTPBearer": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "downloads"
                ],
                "summary": "Post Downloads",
                "operationId": "post_downloads",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/Video-Input"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "201": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Video-Output"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "HTTPBearer": []
                    }
                ]
            }
        },
        "/downloads/{video_id}": {
            "get": {
                "tags": [
                    "downloads"
                ],
                "summary": "Get Download",
                "operationId": "get_download",
                "security": [
                    {
                        "HTTPBearer": []
                    }
                ],
                "parameters": [
                    {
                        "name": "video_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Video Id"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/Video-Output"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "downloads"
                ],
                "summary": "Delete Download",
                "operationId": "delete_download",
                "security": [
                    {
                        "HTTPBearer": []
                    }
                ],
                "parameters": [
                    {
                        "name": "video_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Video Id"
                        }
                    }
                ],
                "responses": {
                    "204": {
                        "description": "Successful Response"
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/downloads/{video_id}/file": {
            "get": {
                "tags": [
                    "downloads"
                ],
                "summary": "Get Download File",
                "operationId": "get_download_file",
                "security": [
                    {
                        "HTTPBearer": []
                    }
                ],
                "parameters": [
                    {
                        "name": "video_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Video Id"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "audio/*": {
                                "schema": {
                                    "type": "file"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/downloads/{video_id}/status": {
            "get": {
                "tags": [
                    "downloads"
                ],
                "summary": "Get Download Status",
                "operationId": "get_download_status",
                "security": [
                    {
                        "HTTPBearer": []
                    }
                ],
                "parameters": [
                    {
                        "name": "video_id",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "title": "Video Id"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/StatusUpdate"
                                }
                            }
                        }
                    },
                    "403": {
                        "description": "Forbidden"
                    },
                    "404": {
                        "description": "Not Found"
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "AudioFormat": {
                "type": "string",
                "enum": [
                    "mp3",
                    "aac",
                    "flac",
                    "m4a",
                    "opus",
                    "wav"
                ],
                "title": "AudioFormat"
            },
            "AudioOptions": {
                "properties": {
                    "format": {
                        "$ref": "#/components/schemas/AudioFormat"
                    }
                },
                "type": "object",
                "required": [
                    "format"
                ],
                "title": "AudioOptions"
            },
            "Channel-Input": {
                "properties": {
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "url": {
                        "anyOf": [
                            {
                                "type": "string",
                                "maxLength": 2083,
                                "minLength": 1,
                                "format": "uri"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Url"
                    },
                    "thumbnail": {
                        "anyOf": [
                            {
                                "type": "string",
                                "maxLength": 2083,
                                "minLength": 1,
                                "format": "uri"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Thumbnail"
                    }
                },
                "type": "object",
                "required": [
                    "name"
                ],
                "title": "Channel"
            },
            "Channel-Output": {
                "properties": {
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "url": {
                        "anyOf": [
                            {
                                "type": "string",
                                "maxLength": 2083,
                                "minLength": 1,
                                "format": "uri"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Url"
                    },
                    "thumbnail": {
                        "anyOf": [
                            {
                                "type": "string",
                                "maxLength": 2083,
                                "minLength": 1,
                                "format": "uri"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Thumbnail"
                    }
                },
                "type": "object",
                "required": [
                    "name",
                    "url",
                    "thumbnail"
                ],
                "title": "Channel"
            },
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "Session": {
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Id"
                    }
                },
                "type": "object",
                "required": [
                    "id"
                ],
                "title": "Session"
            },
            "Status": {
                "type": "string",
                "enum": [
                    "WAITING",
                    "DOWNLOADING",
                    "PROCESSING",
                    "DONE",
                    "ERROR",
                    "UNDEFINED"
                ],
                "title": "Status"
            },
            "StatusUpdate": {
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Id"
                    },
                    "status": {
                        "$ref": "#/components/schemas/Status"
                    }
                },
                "type": "object",
                "required": [
                    "id",
                    "status"
                ],
                "title": "StatusUpdate"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Message"
                    },
                    "type": {
                        "type": "string",
                        "title": "Error Type"
                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
            },
            "Video-Input": {
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Id"
                    },
                    "url": {
                        "type": "string",
                        "maxLength": 2083,
                        "minLength": 1,
                        "format": "uri",
                        "title": "Url"
                    },
                    "options": {
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/AudioOptions"
                            }
                        ],
                        "default": {
                            "format": "mp3"
                        }
                    },
                    "title": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Title"
                    },
                    "duration": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Duration"
                    },
                    "thumbnail": {
                        "anyOf": [
                            {
                                "type": "string",
                                "maxLength": 2083,
                                "minLength": 1,
                                "format": "uri"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Thumbnail"
                    },
                    "channel": {
                        "anyOf": [
                            {
                                "$ref": "#/components/schemas/Channel-Input"
                            },
                            {
                                "type": "null"
                            }
                        ]
                    }
                },
                "type": "object",
                "required": [
                    "id",
                    "url"
                ],
                "title": "Video"
            },
            "Video-Output": {
                "properties": {
                    "id": {
                        "type": "string",
                        "title": "Id"
                    },
                    "url": {
                        "type": "string",
                        "maxLength": 2083,
                        "minLength": 1,
                        "format": "uri",
                        "title": "Url"
                    },
                    "options": {
                        "allOf": [
                            {
                                "$ref": "#/components/schemas/AudioOptions"
                            }
                        ],
                        "default": {
                            "format": "mp3"
                        }
                    },
                    "title": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Title"
                    },
                    "duration": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Duration"
                    },
                    "thumbnail": {
                        "anyOf": [
                            {
                                "type": "string",
                                "maxLength": 2083,
                                "minLength": 1,
                                "format": "uri"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Thumbnail"
                    },
                    "channel": {
                        "anyOf": [
                            {
                                "$ref": "#/components/schemas/Channel-Output"
                            },
                            {
                                "type": "null"
                            }
                        ]
                    }
                },
                "type": "object",
                "required": [
                    "id",
                    "url",
                    "options",
                    "title",
                    "duration",
                    "thumbnail",
                    "channel"
                ],
                "title": "Video"
            }
        },
        "securitySchemes": {
            "HTTPBearer": {
                "type": "http",
                "scheme": "bearer"
            }
        }
    },
    "tags": [
        {
            "name": "session",
            "description": "Session management."
        },
        {
            "name": "search",
            "description": "Search YouTube."
        },
        {
            "name": "downloads",
            "description": "Download management."
        }
    ]
}
