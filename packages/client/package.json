{
	"name": "@yd/client",
	"private": true,
	"version": "1.0.0",
	"license": "MIT",
	"main": "./src/index.ts",
	"scripts": {
		"build": "tsc --project tsconfig.json",
		"check": "tsc --project tsconfig.json --noEmit",
		"generate": "openapi --input ../../apps/api/openapi.json --output ./src/generated --client fetch --indent 2 && git apply ./patches/*.patch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@nicolas-chaulet/openapi-typescript-codegen": "^0.27.9",
		"@yd/config": "workspace:*",
		"typescript": "^5.3.3"
	}
}
