{
  "openapi": "3.1.0",
  "info": {
    "title": "youtube-downloader-api",
    "summary": "API to search and download YouTube videos in various formats.",
    "contact": {
      "name": "Jordan Shatford",
      "url": "https://github.com/jordanshatford/youtube-downloader"
    },
    "license": {
      "name": "MIT License",
      "url": "https://github.com/jordanshatford/youtube-downloader/blob/main/LICENSE"
    },
    "version": "1.0.0"
  },
  "paths": {
    "/search": {
      "get": {
        "tags": [
          "search"
        ],
        "summary": "Get Search",
        "operationId": "get_search",
        "security": [
          {
            "HTTPBearer": []
          }
        ],
        "parameters": [
          {
            "name": "query",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Query"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Video"
                  },
                  "title": "Response Get Search"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/search/next": {
      "get": {
        "tags": [
          "search"
        ],
        "summary": "Get Next Search",
        "operationId": "get_next_search",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/Video"
                  },
                  "type": "array",
                  "title": "Response Get Next Search"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          }
        },
        "security": [
          {
            "HTTPBearer": []
          }
        ]
      }
    },
    "/session": {
      "get": {
        "tags": [
          "session"
        ],
        "summary": "Get Session",
        "operationId": "get_session",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Session"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "session"
        ],
        "summary": "Delete Session",
        "operationId": "delete_session",
        "responses": {
          "204": {
            "description": "Successful Response"
          },
          "403": {
            "description": "Forbidden"
          }
        },
        "security": [
          {
            "HTTPBearer": []
          }
        ]
      }
    },
    "/session/validate": {
      "get": {
        "tags": [
          "session"
        ],
        "summary": "Get Session Validate",
        "operationId": "get_session_validate",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Session"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          }
        },
        "security": [
          {
            "HTTPBearer": []
          }
        ]
      }
    },
    "/downloads": {
      "get": {
        "tags": [
          "downloads"
        ],
        "summary": "Get Downloads",
        "operationId": "get_downloads",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "items": {
                    "$ref": "#/components/schemas/Download"
                  },
                  "type": "array",
                  "title": "Response Get Downloads"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          }
        },
        "security": [
          {
            "HTTPBearer": []
          }
        ]
      },
      "put": {
        "tags": [
          "downloads"
        ],
        "summary": "Put Downloads",
        "operationId": "put_downloads",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DownloadInput"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Download"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        },
        "security": [
          {
            "HTTPBearer": []
          }
        ]
      },
      "post": {
        "tags": [
          "downloads"
        ],
        "summary": "Post Downloads",
        "operationId": "post_downloads",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DownloadInput"
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Download"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        },
        "security": [
          {
            "HTTPBearer": []
          }
        ]
      }
    },
    "/downloads/options": {
      "get": {
        "tags": [
          "downloads"
        ],
        "summary": "Get Downloads Options",
        "operationId": "get_downloads_options",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AvailableDownloadOptions"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          }
        },
        "security": [
          {
            "HTTPBearer": []
          }
        ]
      }
    },
    "/downloads/{download_id}": {
      "get": {
        "tags": [
          "downloads"
        ],
        "summary": "Get Download",
        "operationId": "get_download",
        "security": [
          {
            "HTTPBearer": []
          }
        ],
        "parameters": [
          {
            "name": "download_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Download Id"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Download"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "downloads"
        ],
        "summary": "Delete Download",
        "operationId": "delete_download",
        "security": [
          {
            "HTTPBearer": []
          }
        ],
        "parameters": [
          {
            "name": "download_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Download Id"
            }
          }
        ],
        "responses": {
          "204": {
            "description": "Successful Response"
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/downloads/{download_id}/file": {
      "get": {
        "tags": [
          "downloads"
        ],
        "summary": "Get Download File",
        "operationId": "get_download_file",
        "security": [
          {
            "HTTPBearer": []
          }
        ],
        "parameters": [
          {
            "name": "download_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "title": "Download Id"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "audio/*": {
                "schema": {
                  "type": "file"
                }
              },
              "video/*": {
                "schema": {
                  "type": "file"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AudioFormat": {
        "type": "string",
        "enum": [
          "aac",
          "flac",
          "m4a",
          "mp3",
          "opus",
          "wav"
        ],
        "title": "AudioFormat"
      },
      "AvailableDownloadOptions": {
        "properties": {
          "format": {
            "items": {
              "anyOf": [
                {
                  "$ref": "#/components/schemas/AudioFormat"
                },
                {
                  "$ref": "#/components/schemas/VideoFormat"
                }
              ]
            },
            "type": "array",
            "title": "Format"
          },
          "quality": {
            "items": {
              "$ref": "#/components/schemas/DownloadQuality"
            },
            "type": "array",
            "title": "Quality"
          },
          "embed_metadata": {
            "items": {
              "type": "boolean"
            },
            "type": "array",
            "title": "Embed Metadata"
          },
          "embed_thumbnail": {
            "items": {
              "type": "boolean"
            },
            "type": "array",
            "title": "Embed Thumbnail"
          },
          "embed_subtitles": {
            "items": {
              "type": "boolean"
            },
            "type": "array",
            "title": "Embed Subtitles"
          }
        },
        "type": "object",
        "required": [
          "format",
          "quality",
          "embed_metadata",
          "embed_thumbnail",
          "embed_subtitles"
        ],
        "title": "AvailableDownloadOptions"
      },
      "Channel": {
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "url": {
            "type": "string",
            "maxLength": 2083,
            "minLength": 1,
            "format": "uri",
            "title": "Url"
          },
          "thumbnail": {
            "type": "string",
            "maxLength": 2083,
            "minLength": 1,
            "format": "uri",
            "title": "Thumbnail"
          }
        },
        "type": "object",
        "required": [
          "name",
          "url",
          "thumbnail"
        ],
        "title": "Channel"
      },
      "Download": {
        "properties": {
          "video": {
            "$ref": "#/components/schemas/Video"
          },
          "options": {
            "$ref": "#/components/schemas/DownloadOptions"
          },
          "status": {
            "$ref": "#/components/schemas/DownloadStatus"
          }
        },
        "type": "object",
        "required": [
          "video",
          "options",
          "status"
        ],
        "title": "Download"
      },
      "DownloadInput": {
        "properties": {
          "video": {
            "$ref": "#/components/schemas/Video"
          },
          "options": {
            "$ref": "#/components/schemas/DownloadOptions"
          }
        },
        "type": "object",
        "required": [
          "video",
          "options"
        ],
        "title": "DownloadInput"
      },
      "DownloadOptions": {
        "properties": {
          "format": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/AudioFormat"
              },
              {
                "$ref": "#/components/schemas/VideoFormat"
              }
            ],
            "title": "Format"
          },
          "quality": {
            "$ref": "#/components/schemas/DownloadQuality"
          },
          "embed_metadata": {
            "type": "boolean",
            "title": "Embed Metadata"
          },
          "embed_thumbnail": {
            "type": "boolean",
            "title": "Embed Thumbnail"
          },
          "embed_subtitles": {
            "type": "boolean",
            "title": "Embed Subtitles"
          }
        },
        "type": "object",
        "required": [
          "format",
          "quality",
          "embed_metadata",
          "embed_thumbnail",
          "embed_subtitles"
        ],
        "title": "DownloadOptions"
      },
      "DownloadQuality": {
        "type": "string",
        "enum": [
          "best",
          "worst"
        ],
        "title": "DownloadQuality"
      },
      "DownloadState": {
        "type": "string",
        "enum": [
          "WAITING",
          "DOWNLOADING",
          "PROCESSING",
          "DONE",
          "ERROR"
        ],
        "title": "DownloadState"
      },
      "DownloadStatus": {
        "properties": {
          "state": {
            "$ref": "#/components/schemas/DownloadState"
          },
          "downloaded_bytes": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Downloaded Bytes"
          },
          "total_bytes": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Total Bytes"
          },
          "elapsed": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Elapsed"
          },
          "eta": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Eta"
          },
          "speed": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Speed"
          },
          "postprocessor": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "title": "Postprocessor"
          },
          "progress": {
            "anyOf": [
              {
                "type": "number"
              },
              {
                "type": "null"
              }
            ],
            "title": "Progress",
            "readOnly": true
          }
        },
        "type": "object",
        "required": [
          "state",
          "progress"
        ],
        "title": "DownloadStatus"
      },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": {
              "$ref": "#/components/schemas/ValidationError"
            },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "Session": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          }
        },
        "type": "object",
        "required": [
          "id"
        ],
        "title": "Session"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "integer"
                }
              ]
            },
            "type": "array",
            "title": "Location"
          },
          "msg": {
            "type": "string",
            "title": "Message"
          },
          "type": {
            "type": "string",
            "title": "Error Type"
          }
        },
        "type": "object",
        "required": [
          "loc",
          "msg",
          "type"
        ],
        "title": "ValidationError"
      },
      "Video": {
        "properties": {
          "id": {
            "type": "string",
            "title": "Id"
          },
          "url": {
            "type": "string",
            "maxLength": 2083,
            "minLength": 1,
            "format": "uri",
            "title": "Url"
          },
          "title": {
            "type": "string",
            "title": "Title"
          },
          "duration": {
            "type": "string",
            "title": "Duration"
          },
          "thumbnail": {
            "type": "string",
            "maxLength": 2083,
            "minLength": 1,
            "format": "uri",
            "title": "Thumbnail"
          },
          "channel": {
            "$ref": "#/components/schemas/Channel"
          }
        },
        "type": "object",
        "required": [
          "id",
          "url",
          "title",
          "duration",
          "thumbnail",
          "channel"
        ],
        "title": "Video"
      },
      "VideoFormat": {
        "type": "string",
        "enum": [
          "avi",
          "flv",
          "mkv",
          "mov",
          "mp4",
          "webm"
        ],
        "title": "VideoFormat"
      }
    },
    "securitySchemes": {
      "HTTPBearer": {
        "type": "http",
        "scheme": "bearer"
      }
    }
  },
  "tags": [
    {
      "name": "session",
      "description": "Session management."
    },
    {
      "name": "search",
      "description": "Search YouTube."
    },
    {
      "name": "downloads",
      "description": "Download management."
    }
  ]
}
